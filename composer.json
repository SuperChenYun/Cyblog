{
    "name": "topthink/think",
    "description": "the new thinkphp framework",
    "type": "project",
    "keywords": [
        "framework",
        "thinkphp",
        "ORM"
    ],
    "homepage": "http://thinkphp.cn/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "liu21st",
            "email": "liu21st@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "topthink/framework": "5.1.10",
        "topthink/think-captcha": "2.0.2",
        "topthink/think-mongo": "2.0.11",
        "topthink/think-image": "1.0.7",
        "topthink/think-queue": "2.0.3",
        "topthink/think-migration": "2.0.3",
        "overtrue/wechat": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "overtrue/phplint": "dev-master",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "app\\": "application"
        }
    },
    "extra": {
        "think-path": "thinkphp"
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {"type": "composer", "url": "https://packagist.phpcomposer.com"},
        {"packagist": false}
    ]
}
